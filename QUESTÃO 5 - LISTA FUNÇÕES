#include <stdio.h>
#include<stdlib.h>

typedef struct {

	int n1, n2, n3, n4;

} Numerais;

Numerais leitura (){
	
	Numerais result;
	
	printf ("Informe o primeiro numero: ");
	scanf ("%d", &result.n1);
	
	printf ("Informe o segundo numero: ");
	scanf ("%d", &result.n2);
	
	
	printf ("Informe o terceiro numero: ");
	scanf ("%d", &result.n3);
	
	printf ("Informe o quarto numero: ");
	scanf ("%d", &result.n4); 
	
	return result;
	
}



 main ( ){
	
Numerais result;

result = leitura ( );

printf ("%d", result.n1);
printf ("%d", result.n2);
printf ("%d", result.n3);
printf ("%d", result.n4);
	
	
}

