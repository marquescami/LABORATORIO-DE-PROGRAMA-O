#include <stdio.h>
#include <stdlib.h>

void trocaNumeros (int numeros[]);

int main(){
  	int numeros [2];
    printf ("Informe dois numeros: \n");
    scanf ("%d %d", &numeros[0], &numeros [1]);
	
	trocaNumeros (numeros);
	
	printf("valores %d - %d", numeros[0], numeros[1]);
	 
	return 0;
}
  	
void trocaNumeros (int numeros[]){
	
	int aux = 0;
	aux = numeros[0];	
	numeros[0] = numeros[1];
	numeros[1] = aux;
	
	
}

