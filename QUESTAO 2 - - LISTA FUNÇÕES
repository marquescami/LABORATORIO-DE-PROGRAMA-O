#include <stdio.h>
#include <stdlib.h>

int subtrair (int a, int b, int c){

	int result;
	
	result = ((a-b)-c);
	return result;	
}

int main(int argc, char *argv[]) {
	
		int n1, n2, n3, result;
	
		printf ("Informe tres numeros: \n");
		scanf ("%d %d %d", &n1, &n2, &n3);
	
		result = subtrair (n1, n2, n3);
		printf ("O resultado da subtracao foi: %d", result); 
	
	}
